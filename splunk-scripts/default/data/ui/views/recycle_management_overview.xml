          <query>source="wasteprojectionmodel_actuals_20230721.csv" host="wasteManagement" index="esg_data_set" sourcetype="Waste_management_logs" 
| chart max(recycled) AS recycled max(export_international) AS export_international by financial_year 
| append 
    [| makeresults count=5 
    | streamstats count 
    | eval financial_year=case(count=1,"2021-2022",count=2,"2022-2023",count=3,"2023-2024",count=4,"2024-2025",count=5,"2025-2025",count=6,"2026-2026"), predictive_export_international=case(count=1,"123467",count=2,"223467",count=3,"323467",count=4,"123467",count=5,"023467",count=6,"23467") , predictive_recycled=case(count=1,"3123467",count=2,"3023467",count=3,"2123467",count=4,"4123467",count=5,"6123467",count=6,"7123467") 
